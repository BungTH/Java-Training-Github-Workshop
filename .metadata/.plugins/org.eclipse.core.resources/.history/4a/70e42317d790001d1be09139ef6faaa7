package bung.fruitcalculator;

import java.util.ArrayList;

public class Basket {

	Banana infoBanana = new Banana();
	Orange infoOrange = new Orange();
	
	ArrayList<Basket> arrayBanana;
	ArrayList<Basket> arrayOrange;
	
	public void addBanana() {
	}
	
	public void addOrange() {

	}
	
	public double sumWeight() {
		double totalWeight = 0;
		double weightBanana = infoBanana.getWeight();
		double weightOrange = infoOrange.getWeight();
		
		totalWeight = weightBanana + weightOrange;
		
		for(int i = 0;i < arrayBanana.size();i++) {
			Basket tmpBanana = arrayBanana.get(i);
		}
		for(int i = 0;i < arrayBanana.size();i++) {
			Basket tmpOrange = arrayOrange.get(i);
		}
		
		return totalWeight;
	}
	
}
